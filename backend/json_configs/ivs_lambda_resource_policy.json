{
  "Version": "2012-10-17",
  "Id": "default",
  "Statement": [
    {
      "Sid": "ivs-webrtc-deployment",
      "Effect": "Allow",
      "Principal": {
        "Service": "events.amazonaws.com"
      },
      "Action": "lambda:InvokeFunction",
      "Resource": "arn:aws:lambda:us-east-1:454679031165:function:ivs-ip-register",
      "Condition": {
        "ArnLike": {
          "AWS:SourceArn": "arn:aws:events:us-east-1:454679031165:rule/ip-register"
        }
      }
    }
  ]
}
